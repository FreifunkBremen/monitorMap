h1.ui.header Nodes
	.sub.header
		i.icon.list
		| List
table.ui.table
	thead
		tr
			th Name
			th MAC
			th Status
			th Last Seen
			th Client
			th Map
			th
	tbody
		tr(ng-repeat='item in get(list)')
			td {{key}}
				a(ui-sref='monitormap.detail({id:item.id})') {{item.name}}
			td {{item.mac}}
			td
				i.icon.check.circle(ng-if='item.status',style="color:green")
				i.icon.remove.circle(ng-if='!item.status',style="color:red")
			td {{item.datetime|relativeDate:true}}
			td {{item.client_50+item.client_24}}
			td
				a(ui-sref='monitormap.map({c:"{{item.lat}}\:{{item.lon}}\:16"})')
					i.icon.marker
			td
				a(ui-sref='monitormap.edit({id:item.id})')
					i.icon.edit
	tfoot
		tr
			th(colspan='2')
				| Summe {{ get(list).length}}
			th(colspan='2')
				| Up {{getUpTotal()}}
				| Down {{ get(list).length - getUpTotal()}}
			th(colspan='3')
				| Clients {{getClientsTotal()}}
